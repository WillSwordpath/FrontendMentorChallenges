{
    "compilerOptions": {
      "outDir": "./out/",
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "removeComments": true, 
      "sourceMap": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true
    },
    "exclude": [
      "./node_modules/",
      "node_modules",
      "lib"
    ]
}
